  <itcc-ci:IntroduzioneSuddivisioneRicaviVenditePrestazioniCategoriaAttivita contextRef = "cntxCorr_i">
      &lt;p  style="margin-right:.05pt;text-align:justify"&gt;&lt;span style='font-size:10.0pt;line-height:107%;font-family:"Arial",sans-serif; '&gt; &lt;b&gt;Ricavi per categoria di attività&lt;/b&gt;&lt;/span&gt;&lt;/p&gt;
      &lt;p  style="margin-right:.05pt;text-align:justify;text-autospace: none"&gt;&lt;span style='font-size:10.0pt;line-height:107%;font-family:"Arial",sans-serif'&gt;&amp;#160;&lt;/span&gt;&lt;/p&gt;
