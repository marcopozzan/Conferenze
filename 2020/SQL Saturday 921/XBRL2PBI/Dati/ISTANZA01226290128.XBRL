  <itcc-ci:IntroduzioneBilancioSocietaEsercitaAttivitaDirezioneCoordinamento contextRef = "cntxCorr_i">
      &lt;p  style="margin-right:.05pt;text-align:justify"&gt;&lt;span style='font-size:10.0pt;line-height:107%;font-family:"Arial",sans-serif; '&gt; &lt;b&gt;Eventuale appartenenza a un Gruppo&lt;/b&gt;&lt;/span&gt;&lt;/p&gt;
      &lt;p  style="margin-right:.05pt;text-align:justify;text-autospace: none"&gt;&lt;span style='font-size:10.0pt;line-height:107%;font-family:"Arial",sans-serif; '&gt;&amp;#160;&lt;/span&gt;&lt;/p&gt;
      &lt;p  style="text-autospace:none"&gt;&lt;span style="font-size:10.0pt; line-height:107%;"&gt;La vostra Società appartiene al Gruppo Gommagomma che esercita la direzione e coordinamento tramite la Società &lt;b&gt;gommagomma Holding Srl.&lt;/b&gt;&lt;/span&gt;&lt;/p&gt;
      &lt;p  style="margin-right:.05pt;text-align:justify;text-autospace: none"&gt;&lt;span style='font-size:10.0pt;line-height:107%;font-family:"Arial",sans-serif; '&gt;            &lt;/span&gt;&lt;/p&gt;
      &lt;p &gt;&lt;span style='font-size:10.0pt;line-height:107%;font-family: "Arial",sans-serif;'&gt;Nel seguente prospetto vengono forniti i dati essenziali dell'ultimo bilancio approvato della suddetta Società che esercita la direzione e coordinamento (articolo 2497-&lt;i&gt;bis&lt;/i&gt;, quarto comma, C.c.). &lt;/span&gt;&lt;/p&gt;
      &lt;p &gt;&amp;#160;&lt;/p&gt;
  </itcc-ci:IntroduzioneBilancioSocietaEsercitaAttivitaDirezioneCoordinamento>
  <itcc-ci:IntroduzioneCompensiAnticipazioniCreditiConcessiAmministratoriSindaciImpegniAssuntiLoroConto contextRef = "cntxCorr_i">
